{
  "name": "coverage-istanbul",
  "version": "1.1.1",
  "description": "istanbul tools for test coverage",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc -d",
    "test": "npm run test:unit",
    "test:unit": "mocha 'test/**/*.test.{ts,tsx}'",
    "test:cover": "nyc npm run test:unit",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matmanjs/coverage-istanbul.git"
  },
  "keywords": [
    "istanbul",
    "coverage"
  ],
  "author": "helinjiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matmanjs/coverage-istanbul/issues"
  },
  "homepage": "https://github.com/matmanjs/coverage-istanbul#readme",
  "dependencies": {
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "istanbul": "^0.4.5",
    "istanbul-lib-coverage": "^3.0.0",
    "istanbul-lib-source-maps": "^4.0.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/types": "^7.10.2",
    "@types/chai": "^4.2.12",
    "@types/expect": "^24.3.0",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.1",
    "@types/istanbul-lib-coverage": "^2.0.3",
    "@types/istanbul-lib-source-maps": "^4.0.1",
    "@types/lodash": "^4.14.153",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.3"
  }
}
